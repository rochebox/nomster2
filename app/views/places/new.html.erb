<!-- its the form page for a new place entry-->

<div class="booyah-box col-xs-8 col-xs-offset-2">
  <h1 class="brand-font light-blue-color text-shadow-blue heading-size">Enter Your Place Details...Here!</h1>

		 <div class="form-inputs form-styles nomster-text-font">
		  <%= simple_form_for @place do |f| %>
		      	<%= f.input :name %>
		    	<%= f.input :address %>
		    	<%= f.input :description %>
		    	<br />
		    	<%= f.submit 'Create', class: 'btn btn-primary btn-lg nomster-button-styles' %>
		  <% end %>
		</div>

</div>